//MUX 4X1

`ifndef MUX4T1_SV
	`define  MUX4T1_SV
module MU4t1 
import my_pkg::*;
(
   input [(DATA_WIDTH-1):0] a_i,
	input cuatro,
   input [(DATA_WIDTH-1):0] b_i,
	input [(DATA_WIDTH-1):0] c_i,
	input bit [1:0] sel,
	output logic [(DATA_WIDTH-1):0] out
   
);

assign out = (sel==2'b00) ? a_i : 
				((sel==2'b01) ? cuatro):
				((sel==2'b10) ? b_i):
				((sel==2'b11) ? c_i):
				32'bx)));

endmodule 
`endif